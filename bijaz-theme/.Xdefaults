! urxvt settings
!A
urxvt*geometry:         80x25
urxvt*title:            terminal
urxvt*termName:         rxvt
urxvt*background:       #242424
urxvt*foreground:       white
urxvt*cursorBlink:      true
urxvt*cursorColor:      magenta
urxvt*underlineColor:   #86a2be

antialias=true:hinting=true

! A
urxvt*scrollBar:        false
urxvt*jumpScroll:       false
xterm*scrollTtyOutput: false
!URxvt.visualBell: true
! A
urxvt*inheritPixmap:   true
urxvt*internalBorder:   1
URxvt*externalBorder:    0
urxvt*loginShell:       true
urxvt*saveLines:        15000

! A
urxvt*shading:         30
urxvt*fading:   10

! urxvt-256color settings
!A
urxvt-256color*geometry:         80x25
urxvt-256color*title:            terminal
urxvt-256color*termName:         rxvt
urxvt-256color*background:       #242424
urxvt-256color*foreground:       white
urxvt-256color*cursorBlink:      true
urxvt-256color*cursorColor:      magenta
urxvt-256color*underlineColor:   #86a2be

!A
urxvt-256color*font:             xft:DejaVuSansMono Nerd Font:pixelsize=24
urxvt-256color*boldFont:         xft:DejaVuSansMono Nerd Font:pixelsize=24
!urxvt-256color*boldFont:         xft:DejaVu Sans Mono:pixelsize=24
! A
urxvt-256color*scrollBar:        false
urxvt-256color*jumpScroll:       false
!xterm*scrollTtyOutput: false
!urxvt-256color.visualBell: true
! A
urxvt-256color*inheritPixmap:   true
urxvt-256color*internalBorder:   1
urxvt-256color*externalBorder:    0
urxvt-256color*loginShell:       true
urxvt-256color*saveLines:        15000

! A
urxvt-256color*transparent: true
urxvt-256color*shading:         30
!urxvt-256color*fading:   10

!URxvt*inheritPixmap: true
!URxvt*transparent: true
! URxvt*shading: 0 to 99 darkens, 101 to 200 lightens
!URxvt*shading: 110

URxvt.perl-ext-common: font-size

URxvt.keysym.C-Up:     perl:font-size:increase
URxvt.keysym.C-Down:   perl:font-size:decrease
URxvt.keysym.C-S-Up:   perl:font-size:incglobal
URxvt.keysym.C-S-Down: perl:font-size:decglobal


